<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControlPanelComponent.Mutations.SizerController">
   <content>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="105.0" prefWidth="600.0">
         <children>
            <Label layoutX="14.0" layoutY="32.0" text="Please enter total tuppels :" />
            <TextField fx:id="totalTupplesTextField" layoutX="170.0" layoutY="28.0" onKeyReleased="#checkTotalTuppelsTextField" prefHeight="25.0" prefWidth="212.0" promptText="integer number (positive or negative)" />
            <Label layoutX="271.0" layoutY="6.0" text="Sizer mutation" />
            <Label layoutX="14.0" layoutY="62.0" text="Please enter probability :" />
            <Label fx:id="totalTuppelsErrorLabel" graphicTextGap="10.0" layoutX="384.0" layoutY="32.0" prefHeight="17.0" prefWidth="207.0" text="Error you must enter naturl number !" visible="false">
               <padding>
                  <Insets left="5.0" />
               </padding></Label>
            <Slider fx:id="probabilitySlider" layoutX="169.0" layoutY="64.0" max="1.0" prefHeight="14.0" prefWidth="212.0" showTickLabels="true" showTickMarks="true" />
